{
  "mcpServers": {
    "browsermcp": {
      "command": "npx",
      "args": [
        "@browsermcp/mcp@latest"
      ],
      "description": "Browser MCP for web automation, form filling, and navigation",
      "capabilities": [
        "web_navigation",
        "form_automation",
        "content_extraction",
        "screenshot_capture",
        "dom_manipulation"
      ]
    },
    "redis": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-redis"
      ],
      "env": {
        "REDIS_URL": "redis://localhost:6379"
      }
    }
  },
  "ulp-dynamic": {
    "type": "stdio",
    "command": "cd ~/ && node",
    "args": [
      "mcp-integration/ulp-dynamic-mcp-server.js"
    ],
    "env": {
      "HUB_HTTP_URL": "http://localhost:8081",
      "HUB_WS_URL": "ws://localhost:8081"
    }
  },
  "ollama": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "ollama-mcp-server@latest"
    ],
    "env": {
      "OLLAMA_BASE_URL": "http://localhost:11434"
    }
  },
  "playwright": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "@playwright/mcp@latest"
    ]
  }
}