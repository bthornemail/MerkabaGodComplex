{
  "name": "@ulp/cue-dev-environment",
  "version": "1.0.0",
  "description": "CUE Development Environment with Ollama GPT-OSS Integration for Autonomous AI Development",
  "main": "dev-orchestrator.ts",
  "exports": {
    ".": "./dev-orchestrator.ts",
    "./ollama": "./ollama-agent-protocol.ts",
    "./testing": "./autonomous-testing-framework.ts",
    "./content": "./content-production-agent.ts",
    "./integration": "./peer-integration-testing.ts"
  },
  "scripts": {
    "dev:start": "npx ts-node dev-orchestrator.ts",
    "ollama:test": "npx ts-node ollama-agent-protocol.ts --test",
    "content:generate": "npx ts-node content-production-agent.ts",
    "test:autonomous": "npx ts-node autonomous-testing-framework.ts",
    "peer:test": "npx ts-node peer-integration-testing.ts",
    "env:setup": "npx ts-node setup-dev-environment.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.18.0",
    "dotenv": "^16.3.0",
    "chalk": "^4.1.2",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.16",
    "@types/ws": "^8.5.0",
    "typescript": "^5.8.3",
    "ts-node": "^10.9.0"
  },
  "overrides": {
    "tmp": "^0.2.4",
    "esbuild": "^0.25.8"
  },
  "keywords": [
    "CUE",
    "development",
    "ollama",
    "gpt-oss",
    "autonomous-ai",
    "testing"
  ],
  "author": "Universal Life Protocol Team",
  "license": "ISC"
}
