{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx watch server/main.ts",
    "client": "tsx watch server/client.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently -c \"auto\" -p \"{name}-{pid}\" -n \"Server,Dev\" \"npm run server\" \"npm run dev\"",
    "test": "concurrently -c \"auto\" -p \"{name}-{pid}\" -n \"Server,Client\" \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@types/mqtt": "^2.5.0",
    "@types/three": "^0.175.0",
    "bootstrap": "^5.3.5",
    "ethers": "^6.13.5",
    "graphology": "^0.26.0",
    "graphology-components": "^1.5.4",
    "graphology-shortest-path": "^2.1.0",
    "graphology-types": "^0.24.8",
    "merkletreejs": "^0.5.2",
    "mqtt": "^5.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrency": "^0.1.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
