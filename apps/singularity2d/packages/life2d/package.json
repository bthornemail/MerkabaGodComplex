{
  "name": "@singularity/life2d",
  "version": "1.0.0",
  "scripts": {
    "start": "tsx watch index.ts",
    "dev": "tsx watch proxy.ts",
    "socket": "tsx watch  socket.ts",
    "redis": "tsx watch  redis.proxy.ts",
    "client": "tsx watch  client.interval.ts.message.ts",
    "mqtt": "cd services/mosquitto && sh ./mosquitto.sh",
    "watch": "concurrently  -c \"auto\"  \"yarn:start\" \"yarn:socket\" \"yarn:redis\" \"yarn:client\"",
    "host": "concurrently  -c \"auto\"  \"yarn:start\" \"yarn:client\"",
    "proxy": "concurrently  -c \"auto\"  \"yarn:redis\" \"yarn:client\""
  },
  "dependencies": {
    "@helia/dag-json": "^3.0.3",
    "@helia/json": "^3.0.3",
    "@types/express": "^4.17.21",
    "@types/memjs": "^1.3.3",
    "blockstore-core": "^4.4.1",
    "ethers": "^6.11.1",
    "express": "^4.19.1",
    "feed": "^4.2.2",
    "ioredis": "^5.3.2",
    "marked": "^12.0.1",
    "memcached": "^2.2.2",
    "memjs": "^1.3.2",
    "milliparsec": "^2.3.0",
    "mqtt": "^5.3.5",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "three-spritetext": "^1.8.2"
  }
}
