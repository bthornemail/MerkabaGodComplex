{
  "name": "@vault2d/client",
  "main": "index.ts ",
  "type": "module",
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "relay": "node -r tsconfig-paths/register dist/relay/relay/main.js",
    "webrtc": "node -r tsconfig-paths/register dist/relay/relay/webrtc.js",
    "quic": "node -r tsconfig-paths/register dist/relay/relay/quic.js",
    "dev:relay": "tsc --project tsconfig.relay.json --watch",
    "dev:server": "tsc --project tsconfig.server.json --watch",
    "dev:client": "tsc --project tsconfig.client.json --watch",
    "dev:app": "vite --host --open",
    "dev": "concurrently  -c \"auto\" \"npm run dev:server\" \"npm run dev:relay\" \"npm run dev:client\"  \"npm run dev:app\"",
    "build:relay": "tsc --project tsconfig.relay.json",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc --project tsconfig.client.json",
    "build:app": "vite build",
    "build": "npm run build:server && npm run build:relay && npm run build:client && npm run build:app",
    "pm2": "pm2 start 'npm run dev' --name=vault && pm2 save",
    "c7": "ssh c7 'cd github/vault && git stash && git checkout main && git pull && yarn && pm2 delete all && npm run pm2 && pm2 save && pm2 logs vault'",
    "c8": "ssh c8 'cd github/vault && git stash && git checkout main && git pull && yarn && pm2 delete all && npm run pm2 && pm2 save && pm2 logs vault'",
    "logs": "concurrently  -c \"auto\"  \"ssh c8 'pm2 logs vault'\" \"ssh c8 'pm2 logs vault'\""
  },
  "dependencies": {
    "3d-force-graph": "^1.76.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@ngrok/ngrok": "^1.4.1",
    "axios": "^1.7.9",
    "base64url": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "blockstore-core": "^4.4.1",
    "blockstore-fs": "^1.1.11",
    "blockstore-level": "^1.1.8",
    "body-parser": "^1.20.3",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "d3-force-3d": "^3.0.5",
    "datagram-stream": "^1.1.1",
    "datastore-core": "^9.2.9",
    "datastore-fs": "^9.1.9",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "express-sse": "^0.5.3",
    "front-matter": "^4.0.2",
    "graphology": "^0.25.4",
    "graphology-types": "^0.24.8",
    "hnsw": "^1.0.3",
    "html5-qrcode": "^2.3.8",
    "interface-blockstore": "^5.3.1",
    "interface-datastore": "^8.3.1",
    "ioredis": "^5.4.2",
    "marked": "^12.0.2",
    "memcached": "^2.2.2",
    "merkletreejs": "^0.4.1",
    "mqtt": "^5.10.3",
    "multer": "1.4.5-lts.1",
    "multiformats": "^13.3.1",
    "ngrok": "5.0.0-beta.2",
    "node-quic": "^0.1.3",
    "ollama": "^0.5.12",
    "openpgp": "^5.11.2",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.166.1",
    "three-spritetext": "^1.9.3",
    "tsconfig-paths": "^4.2.0",
    "upnp-client": "^0.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/base64url": "^2.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/live-server": "^1.2.3",
    "@types/memcached": "^2.2.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.16",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.166.0",
    "@types/vcards-js": "^2.10.5",
    "@types/ws": "^8.5.14",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "eslint": "^8.57.1",
