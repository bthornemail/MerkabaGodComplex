{
  "name": "@singularity/universe2d",
  "version": "1.0.0",
  "main": "main.ts",
  "types": "src/types/global.d.ts",
  "type": "module",
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev:build": "concurrently  -c \"auto\" \"npm run dev:types\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:edit": "concurrently  -c \"auto\" \"npm run dev:host\" \"npm run dev:app\"",
    "dev:types": "tsc --project tsconfig.types.json --watch",
    "dev:server": "tsc --project tsconfig.server.json --watch",
    "dev:client": "tsc --project tsconfig.client.json --watch",
    "dev:host": "tsx watch main.ts",
    "dev:app": "vite --host --open",
    "dev": "concurrently  -c \"auto\" \"npm run dev:types\" \"npm run dev:server\" \"npm run dev:host\" \"npm run dev:client\" \"npm run dev:app\"",
    "build:types": "tsc --project tsconfig.types.json",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc --project tsconfig.client.json",
    "build:app": "vite build",
    "build": "npm run build:types && npm run build:server && npm run build:client && npm run build:app",
    "pm2": "pm2 start 'npm run dev' --name=universe2d && pm2 save"
  },
