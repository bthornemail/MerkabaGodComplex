{
  "name": "@ulp/cue-control-center",
  "version": "1.0.0",
  "type": "module",
  "description": "Real-time web UI for monitoring, testing, and configuring CUE framework and protocol harmonization",
  "main": "src/main.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui --passWithNoTests",
    "lint": "eslint .",
    "server:dev": "npx ts-node server/index.ts",
    "server:build": "tsc server/index.ts --outDir dist-server",
    "server:start": "node dist-server/index.js",
    "full-stack:dev": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start": "npm run full-stack:dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@tanstack/react-query": "^5.0.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.263.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.14",
    "@types/express": "^4.17.20",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.8.3",
    "vite": "^6.0.11",
    "vitest": "^1.6.0",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "keywords": [
    "CUE",
    "Control Center",
    "Dashboard",
    "Real-time Monitoring",
    "Protocol Configuration",
    "Universal Life Protocol",
    "Vec7 Harmony",
    "Manuscript Generation"
  ],
  "author": "Universal Life Protocol Team",
  "license": "MIT"
}
